  "private": true,
  "dependencies": {
    "bootstrap": "git://github.com/twbs/bootstrap.git#v4.0.0-alpha.6",
    "font-awesome": "fontawesome#^4.7.0",
    "slick-carousel": "^1.6.0",
    "holderjs": "^2.9.4",
    "opensans": "https://google-fonts.azurewebsites.net/googleFonts/opensans?family=Open+Sans",
    "montserrat": "https://google-fonts.azurewebsites.net/googleFonts/montserrat?family=Montserrat",
    "animate": "animate-scss#^1.2.2",
    "matchHeight": "matchheight#^0.7.2"
  },
  "overrides": {

    "slick-carousel": {
      "main": [
        "./slick/slick.js",
        "./slick/slick.scss"
